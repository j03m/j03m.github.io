  },
  "homepage": "https://github.com/j03m/j03m.github.io",
  "dependencies": {
    "bluebird": "^2.9.24",
    "marked": "^0.3.3",
    "pygmentize-bundled": "^2.3.0",
    "yargs": "^3.7.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "backbone": "^1.2.0",
    "browserify": "^10.1.3",
    "connect-livereload": "^0.5.3",
    "express": "^4.12.3",
    "gulp": "^3.8.11",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul": "^0.10.1",
    "gulp-mocha": "^2.0.1",
    "gulp-react": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sass": "^1.3.3",
    "gulp-tap": "^0.1.3",
    "gulp-util": "^3.0.5",
    "gutil": "^1.6.4",
    "jquery": "^2.1.4",
    "jsdom": "*",
    "jshint-stylish": "^2.0.1",
    "lodash": "^3.8.0",
    "merge-stream": "^0.1.7",
    "mocha": "^2.2.4",
    "moment": "^2.10.3",
    "node-sass": "^2.1.1",
    "react": "^0.13.3",
    "reactify": "^1.1.1",
    "rewire": "^2.3.1",
    "run-sequence": "^1.1.4",
    "through2": "^2.0.0",
    "tiny-lr": "^0.1.5",
    "vinyl-source-stream": "^1.1.0"
  },
  "paths": {
    "tests": "tests/**/*.js",
    "mds": "raws/**/*.md",
    "util": "util/**/*.js",
    "src": "src/**/*.js",
    "sass": "template/**/*.scss",
    "html": "template/**/*.html",
    "css": "template/**/*.css",
    "index": "./page.*.json",
    "entryPoint": "src/app.js",
    "destinations": {
      "dist": "dist",
      "build": "bundle.js"
    }
  },
  "config": {
    "POSTS_PER_PAGE": 10,
    "EXPRESS_PORT": 8000,
    "LIVERELOAD_PORT": 35729,
    "SUMMARY_LENGTH": 720
  }
